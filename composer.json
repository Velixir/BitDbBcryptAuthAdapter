{
  "name": "velixir/bit-db-bcrypt-auth-adapter",
  "description": "Forked BitDbBcryptAuthAdapter from cabrilo/BitDbBcryptAuthAdapter to put create a composer package for it",
  "license": "CUSTOM",
  "keywords": [
    "bcrypt",
    "zf2",
    "authadapter"
  ],
  "config": {
    "vendor-dir": "vendor/BitDbBcryptAuthAdapter"
  },
  "repositories":
  [
    {
      "type": "vcs",
      "version": "1",
      "url": "https://github.com/Velixir/BitDbBcryptAuthAdapter.git"
    }
  ],
  "require": {
    "mnsami/composer-custom-directory-installer": "1.0.*"
  },
  "require-dev": {
    "mnsami/composer-custom-directory-installer": "1.0.*"
  }
}
